            android:layout_width="wrap_content"
            android:id="@+id/txtjenis"
            android:layout_marginLeft="4dp"
            android:textSize="@dimen/font_medium"
            android:textColor="@color/colorDarkBlack"
            android:layout_height="wrap_content"
            />

    </LinearLayout>

    <LinearLayout
