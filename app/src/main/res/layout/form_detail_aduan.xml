            android:textSize="@dimen/font_medium"
            android:textColor="@color/colorDarkBlack"
            android:layout_height="wrap_content"
            />

        <TextView
            android:layout_width="wrap_content"
            android:text=":"
            android:layout_marginLeft="36dp"
            android:textSize="@dimen/font_medium"
            android:textColor="@color/colorDarkBlack"
            android:layout_height="wrap_content"
            />

